{
  "name": "accrefs",
  "version": "1.0.0-alpha",
  "description": "Web Accessiblity References",
  "keywords": [
    "accrefs",
    "a11y",
    "accessiblity"
  ],
  "homepage": "https://github.com/oti/accrefs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oti/accrefs.git"
  },
  "bugs": {
    "url": "https://github.com/oti/accrefs/issues"
  },
  "main": "gulpfile.js",
  "engines": {
    "node": ">=12.7.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.9.0",
    "autoprefixer": "^9.6.1",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.2.0",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.0.2",
    "node-sass-glob-importer": "^5.3.2",
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "watch:eleventy": "eleventy --serve --quiet",
    "watch:styles": "gulp",
    "start": "npm-run-all clean -p watch:*",
    "build:eleventy": "eleventy",
    "build:styles": "gulp build",
    "build": "npm-run-all -s clean build:styles build:eleventy"
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 9",
    "android >= 4",
    "ios >= 6"
  ]
}
